<MudDialog Style=" max-width:500px;">
    <DialogContent>
        <MudText Typo="Typo.h6" Style="font-weight:bold" >Dự án của bản</MudText>
        <MudItem Class="d-flex flex-column py-1 mt-2 mb-2" Style="max-height: 300px; overflow-y: scroll">
            @foreach (var item in items)
            {
                <MudButton OnClick="@((e) => SelectProject(item))">@item.Name</MudButton>
            }
        </MudItem>
    </DialogContent>
</MudDialog>
